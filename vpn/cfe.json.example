{
    "class":"Cloud_Failover",
    "controls":{
      "class":"Controls",
      "logLevel":"silly"
    },
    "environment":"aws",
    "externalStorage":{
      "scopingTags":{
      "f5_cloud_failover_label":"${prefix}-cross-az-stack"
      }
    },
    "failoverAddresses":{
      "scopingTags":{
      "f5_cloud_failover_label":"${prefix}-cross-az-stack"
      }
    },
    "failoverRoutes":{
      "scopingTags":{
      "f5_cloud_failover_label":"${prefix}-cross-az-stack"
      },
      "scopingAddressRanges":[
           {
             "range":"192.168.100.0/24"
           }
       ],
      "defaultNextHopAddresses":{
        "discoveryType":"routeTag"
       }
     }
}
